class nsl_ramp_library : public nsl_library
{
public:
	nsl_ramp_library::nsl_ramp_library();
	nsl_ramp_library::~nsl_ramp_library();

	virtual int run();
};

class nsl_ramp_data_library : public nsl_ramp_library
{
public:
	nsl_ramp_data_library::nsl_ramp_data_library();
	nsl_ramp_data_library::~nsl_ramp_data_library();

  	void NSLramp(nsl_num_0&,const nsl_num_0&);
  	void NSLramp(nsl_num_0&,const nsl_num_0&,const nsl_num_0&);
  	void NSLramp(nsl_num_0&,const nsl_num_0&,const nsl_num_0&,
		const nsl_num_0&,const nsl_num_0&);
};

class nsl_ramp_vector_library : public nsl_ramp_library
{
public:
	nsl_ramp_vector_library::nsl_ramp_vector_library();
	nsl_ramp_vector_library::~nsl_ramp_vector_library();

  	void NSLramp(nsl_num_1&,const nsl_num_1&);
  	void NSLramp(nsl_num_1&,const nsl_num_1&,
		const nsl_num_1&);
  	void NSLramp(nsl_num_1&,const nsl_num_1&,const nsl_num_0&);
  	void NSLramp(nsl_num_1&,const nsl_num_1&,const nsl_num_1&,
		const nsl_num_1&,const nsl_num_1&);
  	void NSLramp(nsl_num_1&,const nsl_num_1&,const nsl_num_1&,
		const nsl_num_0&,const nsl_num_0&);
  	void NSLramp(nsl_num_1&,const nsl_num_1&,const nsl_num_0&,
		const nsl_num_0&,const nsl_num_0&);
};

